<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head') %>
<body>
    <nav>
        <a href="/student/<%= project.createdBy %>">Back to <%= student %> Page</a>
    </nav>
    <h1><%= title %> by <%= student %></h1>
    <img src="<%= project.photo %>" alt="<%= title %>">
    <p>Description: <%= project.description %></p>
    <p>Made on: <%= project.dateMade.toDateString() %></p>
    <a href="/projects/<%= project._id %>/edit" >Edit <%= title %></a>
    <form action="/projects/<%= project._id %>?_method=DELETE" method="POST">
        <input type="submit" value="Delete this Project">
    </form>
    
</body>
</html>